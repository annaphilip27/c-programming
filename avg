// Online C compiler to run C program onl
#include <stdio.h>
int main(){
    int n,i;
  

printf("num of students:");
scanf("%d",&n);
int grade[n];
printf("enter grades of %d\n ",n);

for(i=0;i<n;i++);
{
    printf("grade of %d\n",i+1);
    scanf("%d",&grade[i]);
}
int sum=0;
for(i=0;i<n;i++){
    sum+=grade[i];
    
}
float avg=sum/n;
printf("average grade:%.2f\n",avg);
return 0;

   
}



